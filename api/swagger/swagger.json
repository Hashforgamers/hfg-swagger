{
  "openapi": "3.0.0",
  "info": {
    "title": "HFG Backend API",
    "version": "1.0.0",
    "description": "API documentation for HFG backend"
  },
  "paths": {
    "/api/vendor/{vendor_id}/documents": {
      "get": {
        "summary": "Get Vendor Documents",
        "parameters": [
          {
            "name": "vendor_id",
            "in": "path",
            "required": true,
            "description": "ID of the vendor",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of documents",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/vendor/{vendor_id}/dashboard": {
      "get": {
        "summary": "Get Vendor Dashboard",
        "parameters": [
          {
            "name": "vendor_id",
            "in": "path",
            "required": true,
            "description": "ID of the vendor",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Vendor dashboard data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "total_revenue": {
                      "type": "integer"
                    },
                    "active_sessions": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
